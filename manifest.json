{
  "manifest_version": 2,
  "name": "Get Url From Google",
  "description": "",
  "version": "1.0",

  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },

  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],

  "browser_action": {
    "default_icon": "images/icon/icon_128.png",
    "default_title": "Sample",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": [
        "http://www.google.co.jp/*",
        "https://www.google.co.jp/*"
      ],
      "js": ["js/jquery-2.1.1.min.js", "js/contentscript.js"],
      "run_at": "document_end"
    }
  ]
}